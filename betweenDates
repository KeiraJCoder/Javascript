 // To set two dates to two variables
const birthdate = new Date("08/01/1987");
let today = new Date("06/28/2021");

// To calculate the time difference of two dates
let differenceInTime = today.getTime() - birthdate.getTime();

// To calculate the no. of days between two dates
let differenceInDays = differenceInTime / (1000 * 60 * 60 * 24);

//To display the final no. of days (result)
console.log(`Total number of days between dates ${birthdate} and ${today} is: ${differenceInDays} days`);
